<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
         
	
	<welcome-file-list>
	
			
			<welcome-file>home1.jsp</welcome-file>
			
	</welcome-file-list>
	
	
	
	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>user.loginservlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	
	<servlet>
    	<servlet-name>userinsert</servlet-name>
    	<servlet-class>user.userinsert</servlet-class>
    </servlet>
    <servlet-mapping>
    	<servlet-name>userinsert</servlet-name>
    	<url-pattern>/insert</url-pattern>
    </servlet-mapping>

	<servlet>
		<servlet-name>updateuser</servlet-name>
		<servlet-class>user.updateuserservlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>updateuser</servlet-name>
		<url-pattern>/update</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>deleteuser</servlet-name>
		<servlet-class>user.deleteuserservlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>deleteuser</servlet-name>
		<url-pattern>/delete</url-pattern>
	</servlet-mapping>
	
	<servlet>
    <servlet-name>vehicleinsert</servlet-name>
    <servlet-class>vehicle.vehicleinsert</servlet-class>
	</servlet>
	<servlet-mapping>
    <servlet-name>vehicleinsert</servlet-name>
    <url-pattern>/vinsert</url-pattern>
	</servlet-mapping>
	
	<servlet>
    <servlet-name>updatevehicle</servlet-name>
    <servlet-class>vehicle.updatevehicle</servlet-class>
	</servlet>
	<servlet-mapping>
    <servlet-name>updatevehicle</servlet-name>
    <url-pattern>/vupdate</url-pattern>
	</servlet-mapping>
	
	<servlet>
    <servlet-name>deleteevehicle</servlet-name>
    <servlet-class>vehicle.deletevehicle</servlet-class>
	</servlet>
	<servlet-mapping>
    <servlet-name>deleteevehicle</servlet-name>
    <url-pattern>/vdelete</url-pattern>
	</servlet-mapping>
	

	
	
	
	
	
	
	
	
</web-app>